#HealthBar {
    display: flex;
    flex-direction: column;
    justify-content: space-between;
    align-items: center;
    translate: -50% -50%;

    -unity-font-style: bold;
    color: rgb(181, 210, 248);
    -unity-text-outline-width: 1px;
    -unity-text-outline-color: rgb(11, 60, 123);
}

#HealthBar * {
    padding: 0;
    margin: 0;
}